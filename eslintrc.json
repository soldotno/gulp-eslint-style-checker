{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "consistent-return": 0,
        "no-undef": 0,
        "no-multi-spaces": 0,
        "strict": 0,
        "no-extra-strict": 0,
        "quotes": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "eol-last": 0,
        "no-mixed-spaces-and-tabs": 1,
        "max-len": "80", 
        "new-cap": 0
    }
}
