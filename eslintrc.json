{
  "extends": "airbnb",
  "parser": "babel-eslint",
    "env": {
      "browser": true,
      "jasmine": true,
      "node": true
    },
    "settings": {
      "ecmascript": 6,
      "jsx": true
    },
    "plugins": [
      "react"
    ],
    "globals": {
      "before": true,
      "assert": true,
      "path": true
    },
    "rules": {
      "consistent-return": 0,
      "no-undef": 1,
      "no-multi-spaces": 0,
      "strict": 1,
      "no-extra-strict": 0,
      "space-infix-ops": 0,
      "quotes": 0,
      "no-unused-vars": 1,
      "camelcase": 0,
      "no-underscore-dangle": 0,
      "eol-last": 0,
      "no-mixed-spaces-and-tabs": 1,
      "max-len": [2, 120, 4],
      "key-spacing": [2, {
        "beforeColon": false,
        "afterColon": true
      }],
      "new-cap": 0,
      "strict": 0
    }
}
