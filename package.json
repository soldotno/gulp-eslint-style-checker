{
  "name": "gulp-eslint-style-checker",
  "description": "Performs code style checking using eslint trough gulp",
  "version": "4.0.1",
  "private": false,
  "main": "./lib/eslintChecker.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/soldotno/gulp-eslint-style-checker.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "gulp",
    "Code style checker"
  ],
  "scripts": {
    "prepublish": "babel src/ --out-dir lib/",
    "test": "gulp style-check --config eslintrc.json"
  },
  "dependencies": {
    "babel": "6.5.2",
    "babel-eslint": "7.1.0",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-react": "6.4.1",
    "gulp-eslint": "3.0.1",
    "minimist": "1.2.0"
  },
  "devDependencies": {
    "gulp": "3.9.1"
  }
}
