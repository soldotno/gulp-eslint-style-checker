{
  "name": "gulp-eslint-style-checker",
  "description": "Performs code style checking using eslint trough gulp",
  "version": "4.0.3",
  "private": false,
  "main": "./lib/eslintChecker.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/soldotno/gulp-eslint-style-checker.git"
  },
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "gulp",
    "Code style checker"
  ],
  "scripts": {
    "prepublish": "babel src/ --out-dir lib/",
    "test": "gulp style-check --config eslintrc.json"
  },
  "dependencies": {
    "babel": "5.8.35",
    "babel-eslint": "5.0.0",
    "eslint": "2.3.0",
    "eslint-config-airbnb": "4.0.0",
    "eslint-plugin-react": "3.16.1",
    "gulp-eslint": "1.1.1",
    "minimist": "3.0.3"
  },
  "devDependencies": {
    "gulp": "3.9.0"
  }
}
