{
  "name": "gulp-eslint-style-checker",
  "description": "Performs code style checking using eslint trough gulp",
  "version": "3.1.0",
  "private": false,
  "main": "./lib/eslintChecker.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/soldotno/gulp-eslint-style-checker.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "gulp",
    "Code style checker"
  ],
  "scripts": {
    "prepublish": "babel src/ --out-dir lib/",
    "test": "gulp style-check --config eslintrc.json"
  },
  "dependencies": {
    "babel": "5.8.29",
    "babel-eslint": "4.1.3",
    "eslint": "1.7.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-plugin-react": "3.6.3",
    "gulp-eslint": "1.0.0",
    "minimist": "1.2.0"
  },
  "devDependencies": {
    "gulp": "3.9.0"
  }
}
